{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "ruby-ui": {
      "command": "npx",
      "args": [
        "-y",
        "sitemcp",
        "https://rubyui.com",
        "-m",
        "/docs/**"
      ]
    }
  }
}
